package Sep22.week1.day2;

public class PrimeNumber {

	public static void main(String[] args) {
		int Input=89;
		boolean Flag=false;
		
		for (int i=2;i<=Input/2;i++){	
		if(Input%i==0){
				Flag=true;
		break;
	}
	}
	
		if (Flag==false)
		System.out.println("Prime Number");
	
		else
		System.out.println("Not a Prime Number");
	}

}
